<h2>Publishers</h2>
<table>
 <thead>
   <tr>
     <th>Publisher Name</th>
     <th>   </th>
     <th>Actions</th>
   </tr>
 </thead>


   <% @publishers.each do |publisher| %>
     <tr>
       <td><%= publisher.name %></td>
       <td>   </td>
       <td><%= link_to 'Edit', edit_publisher_path(publisher.id) %>  |  <%= link_to 'Delete' , publisher_path(publisher), method: :delete, data: {confirm: 'Are you sure?'} %></td>
     </tr>
   <% end %>

</table>
